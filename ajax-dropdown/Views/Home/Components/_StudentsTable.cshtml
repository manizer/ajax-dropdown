@model ajax_dropdown.ViewModels.HomeViewModels.Components.StudentsTableViewModel
@{
    ViewData["Title"] = "_StudentsTable";
}

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nama</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Students)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nama</td>
            </tr>
        }
    </tbody>
</table>